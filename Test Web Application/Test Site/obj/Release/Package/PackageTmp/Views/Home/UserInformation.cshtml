@model Test_Site.Models.User
@{
    ViewBag.Title = "User Iformation";
}

<div class="container">

	<div class="row">
		<h2>User Information</h2>
	</div>

	<div class="row">
		<div class="col-md-3">Name:</div>
		<div class="col-lg-9">
			@Model.GetName()
		</div>

		<div class="col-md-3">Age:</div>
		<div class="col-lg-9">
			@Model.Age
		</div>

		<div class="col-md-3">Email address:</div>
		<div class="col-lg-9">
			@Model.EmailAddress
		</div>
	</div>

</div>






